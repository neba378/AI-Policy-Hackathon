{
  "name": "ai-docs-scraper",
  "version": "1.0.0",
  "description": "Automated AI documentation scraper with semantic embeddings and MongoDB storage",
  "main": "start-ai-scraper-direct.js",
  "scripts": {
    "start": "node start-ai-scraper-direct.js",
    "start:all": "node start-ai-scraper-direct.js --all",
    "start:openai": "node start-ai-scraper-direct.js --openai",
    "start:anthropic": "node start-ai-scraper-direct.js --anthropic",
    "start:google": "node start-ai-scraper-direct.js --google",
    "start:meta": "node start-ai-scraper-direct.js --meta",
    "start:test": "node start-ai-scraper-direct.js --test",
    "sources": "node show-sources.js",
    "query": "node query-database.js",
    "analyze": "python run_analysis.py",
    "analyze:extract": "python data_analysis/scripts/extract_mongodb_data.py"
  },
  "keywords": [
    "ai",
    "documentation",
    "scraper",
    "embeddings",
    "mongodb",
    "semantic-search",
    "vector-database",
    "llm",
    "openai",
    "anthropic"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@xenova/transformers": "^2.17.1",
    "axios": "^1.6.5",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.6.1",
    "mongodb": "^6.21.0",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^24.29.1",
    "winston": "^3.11.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}